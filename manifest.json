{
    "manifest_version": 3,
    "name": "Gmail Privacy",
    "version": "1.0",
    "description": "Hides email previews and subject lines in Gmail",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://mail.google.com/*"],
        "css": ["styles.css"]
      }
    ]
  }
  